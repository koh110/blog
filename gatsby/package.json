    "gatsby-plugin-twitter": "^5.0.0",
    "gatsby-plugin-typography": "^5.0.0",
    "gatsby-remark-autolink-headers": "^6.0.0",
    "gatsby-remark-copy-linked-files": "^6.0.0",
